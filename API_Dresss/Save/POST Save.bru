meta {
  name: POST Save
  type: http
  seq: 1
}

post {
  url: {{ base_url }}/api/saves
  body: json
  auth: inherit
  headers: {
    "Authorization": "Bearer {{ jwt }}",
    "Content-Type": "application/json"
  }
}

body:json {
  {
    "userId": "{{ user_id }}",
    "itemId": "{{ item_id }}",
    "itemType": "{{ item_type }}"
  }
}
